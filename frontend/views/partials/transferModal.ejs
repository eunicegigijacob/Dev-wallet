<div class="modal"  id="modalTransfer" tabindex="-1">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Transfer</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close" id="trans-close-btn"></button>
        </div>
        <div class="modal-body">
          <form class="forms-sample" id="transfer-form">
            <div class="form-group">
                <label for="beneficiary-name">Beneficiary Username</label>
                <input type="text" class="form-control" id="beneficiary-name" placeholder="Beneficiery username...">
              </div>
            <div class="form-group">
              <label for="trans-amount">Amount</label>
              <input type="text" class="form-control" id="trans-amount" placeholder="amount...">
            </div>
            <div class="form-group">
              <label for="transferPin"> Password</label>
              <input type="password" class="form-control" id="transfer-Pin" placeholder="password">
            </div>
         
     
            <button type="submit" class="btn btn-primary me-2" id="submitT-btn">Submit</button>
            <button class="btn btn-light">Cancel</button>
          </form>
        </div>
        <div class="modal-footer">
  
        </div>
      </div>
    </div>
  </div>
  